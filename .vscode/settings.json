{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "cSpell.words": [
      "codenames",
      "colour",
      "commitlint",
      "datacentres",
      "devs",
      "Eagleson",
      "esbenp",
      "Etyo",
      "Kasper",
      "leerob",
      "nextauth",
      "nextjs",
      "npmrc",
      "pravatar",
      "preload",
      "prettierignore",
      "prettierrc",
      "regexes",
      "rgba",
      "scaleable",
      "signin",
      "Supabase",
      "superpowered",
      "tailwindcss",
      "typeof",
      "typesafe",
      "unoptimized",
      "userinfo",
      "Vercel",
      "Viewports",
      "WEBP"
    ],
    "[javascriptreact]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[javascript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
    }
  }